        <jakarta.webservice.version>3.1.0</jakarta.webservice.version>
        <jakarta.xml.webservice.version>4.0.0</jakarta.xml.webservice.version>
        <jakarta.xml.binding.version>4.0.0</jakarta.xml.binding.version>
        <jaxb.version>4.0.3</jaxb.version>
