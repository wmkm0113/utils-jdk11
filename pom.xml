        </dependency>
        <dependency>
            <groupId>org.eclipse.angus</groupId>
            <artifactId>dsn</artifactId>
            <version>${angus.version}</version>
            <exclusions>
                <exclusion>
                    <groupId>org.eclipse.angus</groupId>
                    <artifactId>jakarta.mail</artifactId>
