    <packaging>jar</packaging>
    <name>Nervousync Java Utility Collections</name>
    <url>https://gene.nervousync.org/index.shtml</url>
    <description>Java utility collections, development by Nervousync Studio (NSYC)</description>
    <issueManagement>
        <url>https://github.com/wmkm0113/Gene/issues</url>
        <system>Github Issues</system>
    </issueManagement>
    <licenses>
        <license>
            <name>The Apache Software License, Version 2.0</name>
            <url>https://www.apache.org/licenses/LICENSE-2.0.txt</url>
            <distribution>repo</distribution>
        </license>
    </licenses>
    <scm>
        <tag>v1.2.0</tag>
        <url>https://github.com/wmkm0113/utils-jdk11</url>
        <connection>scm:git:https://github.com/wmkm0113/utils-jdk11.git</connection>
        <developerConnection>scm:git:https://github.com/wmkm0113/utils-jdk11.git</developerConnection>
    </scm>
    <organization>
        <url>https://nervousync.org/</url>
        <name>Nervousync Studio</name>
    </organization>
    <developers>
        <developer>
            <name>Steven Wee</name>
            <email>wmkm0113@gmail.com</email>
            <url>https://nervousync.org/member/wmkm0113</url>
            <organization>Nervousync Studio</organization>
            <organizationUrl>https://nervousync.org/index.shtml</organizationUrl>
