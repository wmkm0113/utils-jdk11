        </dependency>
        <dependency>
            <groupId>org.bouncycastle</groupId>
            <artifactId>bcjmail-jdk18on</artifactId>
            <version>${bcprov.version}</version>
            <exclusions>
                <exclusion>
                    <groupId>com.sun.mail</groupId>
